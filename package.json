{
  "name": "testcafe-testing",
  "version": "1.0.0",
  "description": "Automation testing with Testcafe",
  "main": "index.js",
  "scripts": {
    "test:chrome": "testcafe chrome ./tests -s takeOnFails=true --disable-page-caching --reporter list",
